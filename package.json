{
  "name": "svg-visual-bbox-toolkit",
  "version": "1.0.0",
  "description": "High-precision visual bounding boxes and SVG object tooling powered by headless Chrome & Puppeteer",
  "type": "module",
  "keywords": [
    "svg",
    "bounding-box",
    "bbox",
    "puppeteer",
    "visual-bounds",
    "svg-tools",
    "headless-chrome"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "ci": "pnpm run lint && pnpm run typecheck && pnpm run test:coverage && pnpm run test:e2e",
    "install-browsers": "playwright install --with-deps chromium && npx puppeteer browsers install chrome"
  },
  "dependencies": {
    "chrome-launcher": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^9.9.0",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^14.12.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.3.0",
    "puppeteer": "^24.31.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
